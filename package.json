{
    "name": "longboard-sl",
    "version": "1.0.0-alpha.1",
    "description": "Electric Longboards Service Layer",
    "main": "server.ts",
    "private": true,
    "scripts": {
        "build": "gulp",
        "lint": "gulp lint",
        "clean": "gulp clean",
        "start": "gulp start",
        "test": "nyc ./node_modules/.bin/mocha",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "junit": "nyc ./node_modules/.bin/mocha --reporter=mocha-junit-reporter",
        "sonar": "gulp sonar"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "exclude": [
            "**/*.js",
            "**/*.d.ts"
        ],
        "reporter": [
            "html",
            "text",
            "lcov",
            "cobertura",
            "text-summary"
        ],
        "all": true,
        "cache": true,
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/speedboard/longboard.git"
    },
    "keywords": [
        "skate",
        "electric",
        "longboard"
    ],
    "author": "Ismael Queiroz <eu@iqueiroz.com.br>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/speedboard/longboard/issues"
    },
    "homepage": "https://speedboard.github.io",
